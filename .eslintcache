lcome.tsx",[],"/Users/mikhailchukhnin/Desktop/repo/3dschool/src/components/Slide.tsx",[],["151","152"],"/Users/mikhailchukhnin/Desktop/repo/3dschool/src/components/index.ts",[],"/Users/mikhailchukhnin/Desktop/repo/3dschool/src/components/Video.tsx",[],"/Users/mikhailchukhnin/Desktop/repo/3dschool/src/components/Title.tsx",[],"/Users/mikhailchukhnin/Desktop/repo/3dschool/src/components/Label.tsx",[],"/Users/mikhailchukhnin/Desktop/repo/3dschool/src/components/Flex/Col.tsx",[],"/Users/mikhailchukhnin/Desktop/repo/3dschool/src/components/Flex/Row.tsx",[],"/Users/mikhailchukhnin/Desktop/repo/3dschool/src/components/Flex/Cell.tsx",[],"/Users/mikhailchukhnin/Desktop/repo/3dschool/src/components/Container.tsx",[],"/Users/mikhailchukhnin/Desktop/repo/3dschool/src/components/PlayButton.tsx",["153"],"import * as React from 'react';\nimport { useModal } from './Modal';\nimport { YouTubeDialog } from './YoutubeDialog';\n\ninterface IProps {\n    link: string\n    style?: React.CSSProperties\n}\n\nexport const PlayButton: React.FC<IProps> = (props) => {\n    const { link, style = {} } = props;\n    const { call } = useModal();\n\n    return (\n        <div style={{\n            ...style,\n            width: 80,\n            height: 80,\n            margin: \"auto\",\n            backgroundColor: \"#FF2747\",\n            borderRadius: \"50%\",\n            position: \"relative\",\n            cursor: \"pointer\",\n        }}\n            onClick={() => call(<YouTubeDialog src={\"https://www.youtube.com/embed/2GoexbWjJys\"} />)}\n        >\n            <img style={{\n                top: 10,\n                left: 10,\n                position: \"absolute\"\n            }} src=\"images/play.png\" alt=\"\" />\n        </div>\n    )\n}","/Users/mikhailchukhnin/Desktop/repo/3dschool/src/components/Flex/index.tsx",[],"/Users/mikhailchukhnin/Desktop/repo/3dschool/src/components/Section.tsx",[],"/Users/mikhailchukhnin/Desktop/repo/3dschool/src/Slides/Study.tsx",["154"],"\nimport { Image } from '../components/Image';\nimport { Title } from '../components/Title';\nimport { Label } from '../components/Label';\nimport { Col, Row } from '../components/Flex';\nimport { Container } from '../components/Container';\nimport { PlayButton } from '../components/PlayButton';\nimport { Section } from '../components/Section';\nimport { Carousel } from '../components/Carousel';\nimport * as React from 'react';\nimport { Hidden } from '../components/Hidden';\n\nconst content = [\n    [\"Моделирование\", \"thumb_model\",\n        `   Моделирование лежит в основе всех существующих 3D-технологий! Поэтому начать свое знакомство с миром 3D стоит имено с него.\n\n    Во время обучния нас ждет:\n-изучение современных инструментов и приемов моделирования;\n-создание собственных 3D-объектов и персонажей;\n-подготовка моделей для 3D-печати и видео-игр.\n\n    Все это подарит ребенку новые возможности, раскроет его творческийпотенциал и даже сможет перерасти в полноценый способ самореализации!`],\n    [\"3D-печать\", \"thumb_3d_print\",\n        `   Современое производство не обходится без 3D-печати.\u0003 \nПоэтому мы создали курс, который познакомит ребенка со всеми этапами печати и научит создавать собственые изделия!\n\n    Во время обучния нас ждет:\n-знакомство с устройством 3D-принтера;\n-разработка функциональных механизмов и устройств;\n-работа с 3D-принтером при создании собственных изделий.\n\n    Данный курс позволит ребенку осознать свой безграничный потенциал, и почувствовать себя настоящим творцом! `],\n    [\"Кинопроизводство\", \"thumb_film_making\",\n        `   3D-графика является неотъемлемой частью Кинематографа.\nПоэтому мы разработали курс, который поможет ребенку прикоснуться\u0003\nк передовым 3D-технологиям кинопроизводства.\n\n    Во время обучния нас ждет:\n-знакомство с основными приемами кинопроизводства;\n-практическая работа с камерой, хромакеем и освещением;\n-использование дополненной реальности для прямых эфиров. \n    \n    Все это позволит ребенку погрузиться в Кинопроизводство, \u0003\nчто послужит хорошим стартом для дальнейшего развития в этой сфере.`],\n    [\"VR-разработка\", \"thumb_vr\",\n        `   Сегодня создание VR-приложений  стало востребованным направлением!\u0003\nЕго используют как на телевидении, так и в сфере эвентов.\u0003\nПоэтому мы подготовили курс, который познакомит ребенка с процессами разработки игровых приложений, а так же сферами их применения.\n\n    Во время обучния нас ждет:\n-знакомство с игровым движком (Unreal Engine 4);\n-сборка собственных миров и сказочных локаций;\n-разработка уникальных игровых приложений.\n    \n    Это направление развовьет в ребенке увереность и креативный подход\u0003\nк решению задач при создании и организации собственых проектов!`]\n]\n\nexport const Study: React.FC = () => (\n    <div style={{ color: \"white\" }}>\n        <Image src=\"study\" alt=\"slide background\" fullScreen />\n        <Section top={40}>\n            <Container size=\"md\" style={{ overflow: \"hidden\" }}>\n                <Col s={16}>\n                    <Title>\n                        Какие дисциплины\n                    <br />\n                    мы изучаем!\n                </Title>\n                    <Carousel\n                        titles={content.map(c => c[0])}\n                    >\n                        {content.map(([, icon, text], index) => (\n                            <Row m={8} s={32} key={index}>\n                                <Hidden less=\"sm\" >\n                                    <Image src={icon} alt=\"\" />\n                                </Hidden>\n                                <Label>{text}</Label>\n                            </Row>\n                        ))}\n                    </Carousel>\n                </Col>\n            </Container>\n        </Section>\n    </div>\n)","/Users/mikhailchukhnin/Desktop/repo/3dschool/src/components/Image.tsx",[],"/Users/mikhailchukhnin/Desktop/repo/3dschool/src/components/Carousel/Carousel.tsx",[],"/Users/mikhailchukhnin/Desktop/repo/3dschool/src/components/Carousel/index.tsx",[],"/Users/mikhailchukhnin/Desktop/repo/3dschool/src/components/Carousel/Slides.tsx",[],"/Users/mikhailchukhnin/Desktop/repo/3dschool/src/Slides/Classes.tsx",[],"/Users/mikhailchukhnin/Desktop/repo/3dschool/src/Slides/Sketches.tsx",["155"],"\nimport { Image } from '../components/Image';\nimport { Title } from '../components/Title';\nimport { Label } from '../components/Label';\nimport { Col, Row } from '../components/Flex';\nimport { Container } from '../components/Container';\nimport { PlayButton } from '../components/PlayButton';\nimport { Section } from '../components/Section';\nimport { Carousel } from '../components/Carousel';\nimport * as React from 'react';\n\nconst content = [\n    [\"Скетч проекта 1\", \"sketch_1\",\n        `   Моделирование лежит в основе всех существующих 3D-технологий! Поэтому начать свое знакомство с миром 3D стоит имено с него.\n\n    Во время обучния нас ждет:\n-изучение современных инструментов и приемов моделирования;\n-создание собственных 3D-объектов и персонажей;\n-подготовка моделей для 3D-печати и видео-игр.\n\n    Все это подарит ребенку новые возможности, раскроет его творческийпотенциал и даже сможет перерасти в полноценый способ самореализации!`],\n    [\"Скетч проекта 2\", \"study\",\n        `   Современое производство не обходится без 3D-печати.\u0003 \nПоэтому мы создали курс, который познакомит ребенка со всеми этапами печати и научит создавать собственые изделия!\n\n    Во время обучния нас ждет:\n-знакомство с устройством 3D-принтера;\n-разработка функциональных механизмов и устройств;\n-работа с 3D-принтером при создании собственных изделий.\n\n    Данный курс позволит ребенку осознать свой безграничный потенциал, и почувствовать себя настоящим творцом! `],\n    [\"Скетч проекта 3\", \"sketch_1\",\n        `   3D-графика является неотъемлемой частью Кинематографа.\nПоэтому мы разработали курс, который поможет ребенку прикоснуться\u0003\nк передовым 3D-технологиям кинопроизводства.\n\n    Во время обучния нас ждет:\n-знакомство с основными приемами кинопроизводства;\n-практическая работа с камерой, хромакеем и освещением;\n-использование дополненной реальности для прямых эфиров. \n    \n    Все это позволит ребенку погрузиться в Кинопроизводство, \u0003\nчто послужит хорошим стартом для дальнейшего развития в этой сфере.`],\n    [\"Скетч проекта 4\", \"study\",\n        `   Сегодня создание VR-приложений  стало востребованным направлением!\u0003\nЕго используют как на телевидении, так и в сфере эвентов.\u0003\nПоэтому мы подготовили курс, который познакомит ребенка с процессами разработки игровых приложений, а так же сферами их применения.\n\n    Во время обучния нас ждет:\n-знакомство с игровым движком (Unreal Engine 4);\n-сборка собственных миров и сказочных локаций;\n-разработка уникальных игровых приложений.\n    \n    Это направление развовьет в ребенке увереность и креативный подход\u0003\nк решению задач при создании и организации собственых проектов!`]\n]\n\nexport const Sketches: React.FC = () => {\n    const [index, setIndex] = React.useState(0)\n\n    return (\n        <div style={{ color: \"white\" }}>\n            <Image src={content[index][1]} alt=\"sketch image\" fullScreen />\n            <Section top={40}>\n                <Container size=\"md\" style={{ overflow: \"hidden\" }}>\n                    <Col s={16}>\n                        <Title>\n                            Какие дисциплины\n                        <br />\n                        мы изучаем!\n                    </Title>\n                        <Carousel\n                            onChange={setIndex}\n                            titles={content.map(c => c[0])}\n                        >\n                            {content.map(([, , text], index) => (\n                                <Row m={8} key={index}>\n                                    <Label>{text}</Label>\n                                </Row>\n                            ))}\n                        </Carousel>\n                    </Col>\n                </Container>\n            </Section>\n        </div>\n    )\n}","/Users/mikhailchukhnin/Desktop/repo/3dschool/src/Slides/Workshop.tsx",[],"/Users/mikhailchukhnin/Desktop/repo/3dschool/src/components/Modal/Modal.tsx",["156"],"import { debug } from 'console';\nimport * as React from 'react';\nimport { Row } from '../Flex';\n\ninterface IModal {\n    call: (content: React.ReactNode) => void\n}\n\nconst ModalContext = React.createContext({} as IModal);\nconst { Provider } = ModalContext;\n\nexport const Modal: React.FC = (props) => {\n    const { children } = props;\n    const [isOpened, setOpen] = React.useState(false);\n    const [state, setState] = React.useState<React.ReactNode | null>(null);\n\n    console.log(isOpened)\n\n    const call = (node: React.ReactNode) => {\n        setOpen(true);\n        setState(node);\n    }\n\n    const close = () => {\n        setState(null);\n        setOpen(false);\n    }\n\n    return (\n        <>\n            <Provider value={{\n                call\n            }}>\n                {children}\n            </Provider>\n            <ModalContent opened={isOpened} onClose={close}>\n                {state}\n            </ModalContent>\n        </>\n    )\n}\n\ninterface IProps {\n    opened: boolean\n    onClose: () => void\n}\n\nconst ModalContent: React.FC<IProps> = (props) => {\n    const { opened, children, onClose } = props;\n\n    return (\n        <div style={{\n            position: \"fixed\",\n            left: opened ? 0 : \"50%\",\n            top: opened ? 0 : \"50%\",\n            width: opened ? \"100vw\" : \"0\",\n            height: opened ? \"100vh\" : \"0\",\n            transition: \"all 0.2s\"\n        }} onClick={onClose}>\n            <Row align=\"center\" justify=\"center\" style={{height: \"100%\"}}>\n                {children}\n            </Row>\n        </div>\n    )\n}\n\nexport c